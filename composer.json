{
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "glpi-project/tools": "^0.7.7"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.99"
        },
        "sort-packages": true
    },
    "autoload-dev": {
        "psr-4": {
            "GlpiPlugin\\{$UNAME}\\": "src/",
            "GlpiPlugin\\{$UNAME}\\Tests\\": "tests/"
        }
    }
}
